<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>get-10-1990s-episodes</name>
    <match>all</match>
    <rule field="airdate" operator="after">1989-12-31</rule>
    <rule field="airdate" operator="before">2000-01-01</rule>
    <limit>10</limit>
    <order direction="ascending">random</order>
</smartplaylist>
