<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Recent BestPic</name>
    <match>all</match>
    <rule field="playlist" operator="is">
        <value>Best Picture</value>
    </rule>
    <limit>25</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>
