<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>get-10-1950s-episodes</name>
    <match>all</match>
    <rule field="airdate" operator="after">1949-12-31</rule>
    <rule field="airdate" operator="before">1960-01-01</rule>
    <limit>10</limit>
    <order direction="ascending">random</order>
</smartplaylist>
