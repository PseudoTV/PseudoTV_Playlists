<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Disney Movies</name>
    <match>all</match>
    <rule field="studio" operator="contains">
        <value>disney</value>
    </rule>
    <limit>25</limit>
    <order direction="ascending">random</order>
</smartplaylist>
