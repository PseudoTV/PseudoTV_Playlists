<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Romance TV</name>
    <match>all</match>
    <rule field="genre" operator="is">Romance</rule>
    <rule field="genre" operator="isnot">Comedy</rule>
    <rule field="genre" operator="isnot">Action</rule>
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
