<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>get-100-film-noir-movie</name>
    <match>all</match>
    <rule field="genre" operator="is">Film-Noir</rule>
    <rule field="time" operator="greaterthan">0</rule>
    <rule field="videoresolution" operator="lessthan">720</rule>
    <limit>100</limit>
    <order direction="ascending">random</order>
</smartplaylist>
