<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>get-5-current-episodes-last-1-week</name>
    <match>all</match>
    <rule field="airdate" operator="inthelast">1 week</rule>
    <limit>5</limit>
    <order direction="ascending">airdate</order>
</smartplaylist>