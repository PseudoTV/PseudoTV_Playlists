<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Silver</name>
    <match>all</match>
    <rule field="genre" operator="is">Suspense</rule>
    <rule field="genre" operator="is">Thriller</rule>
    <rule field="genre" operator="isnot">Comedy</rule>
    <rule field="genre" operator="isnot">Horror</rule>
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
