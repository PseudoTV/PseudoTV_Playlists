<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>get-100-current-episodes-last-1-weeks</name>
    <match>all</match>
    <rule field="airdate" operator="inthelast">1 weeks</rule>
    <limit>100</limit>
    <order direction="ascending">airdate</order>
</smartplaylist>