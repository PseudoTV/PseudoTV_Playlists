<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Starz Movies</name>
    <match>all</match>
    <rule field="genre" operator="contains">Music</rule>
    <rule field="genre" operator="isnot">Horror</rule>
    <rule field="genre" operator="isnot">Animation</rule>
    <rule field="genre" operator="isnot">Crime</rule>
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
