<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>get-25-1960s-episodes</name>
    <match>all</match>
    <rule field="airdate" operator="after">1959-12-31</rule>
    <rule field="airdate" operator="before">1970-01-01</rule>
    <limit>25</limit>
    <order direction="ascending">random</order>
</smartplaylist>
