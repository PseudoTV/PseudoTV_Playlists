<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>TrekTV</name>
    <match>all</match>
    <rule field="tvshow" operator="startswith">Star Trek</rule>
    <limit>150</limit>
    <rule field="lastplayed" operator="notinthelast">2 weeks</rule>
    <order direction="ascending">random</order>
</smartplaylist>
