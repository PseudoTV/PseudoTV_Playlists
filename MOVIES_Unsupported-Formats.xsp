<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Unsupported Movie Formats</name>
    <match>one</match>
    <rule field="filename" operator="contains">.flv</rule>
    <rule field="filename" operator="contains">.mov</rule>
    <rule field="filename" operator="contains">.mpg</rule>
</smartplaylist>
