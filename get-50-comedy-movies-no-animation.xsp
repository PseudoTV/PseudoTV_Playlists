<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>get-50-comedy-movies-no-animation</name>
    <match>all</match>
    <rule field="genre" operator="is">Comedy</rule>
    <rule field="genre" operator="isnot">Animation</rule>
    <rule field="time" operator="greaterthan">0</rule>
    <rule field="videoresolution" operator="lessthan">720</rule>
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
