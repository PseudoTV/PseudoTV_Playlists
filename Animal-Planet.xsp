<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>Animal Planet</name>
    <match>all</match>
    <rule field="genre" operator="is">
        <value>Animal</value>
    </rule>
    <rule field="path" operator="contains">
        <value>animal</value>
    </rule>
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
