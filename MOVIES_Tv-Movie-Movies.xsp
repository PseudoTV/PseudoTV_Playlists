<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>TV Movie Movies</name>
    <match>one</match>
    <rule field="genre" operator="is">
        <value>TV Movie</value>
    </rule>
    <limit>25</limit>
    <order direction="ascending">random</order>
</smartplaylist>
