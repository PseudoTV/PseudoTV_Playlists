<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>The Comedy Channel</name>
    <match>all</match>
    <rule field="year" operator="lessthan">2000</rule>
    <rule field="genre" operator="is">Comedy</rule>
    <rule field="genre" operator="doesnotcontain">sci</rule>
    <rule field="genre" operator="isnot">Animation</rule>
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
