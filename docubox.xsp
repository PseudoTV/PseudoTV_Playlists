<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>Docubox</name>
    <match>all</match>
    <rule field="genre" operator="is">Documentary</rule>
    <rule field="genre" operator="doesnotcontain">Show</rule>
    <limit>150</limit>
    <rule field="lastplayed" operator="notinthelast">3 weeks</rule>
    <order direction="ascending">random</order>
</smartplaylist>
